<script>
  const sectionBranchElement = document.querySelector(".select_branch");
  const applyGradeElement = document.querySelector(".apply_grade");
  sectionBranchElement.addEventListener("change", async (e) => {
    let res = await fetch(`/branch/${e.target.value}`, {
      method: "GET",
    });
    let data = await res.json();
    applyGradeElement.innerHTML = `<option selected disabled>
                    <%= admissionLanguage.admission_form.form.grade[lan] %>
                  </option>`
    data.data.branch.branch_grades.forEach((element) => {
      let option = document.createElement("option");
      option.value = element;
      option.textContent = element;
      applyGradeElement.appendChild(option);
    });
  });
</script>

