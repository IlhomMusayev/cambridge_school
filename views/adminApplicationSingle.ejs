            </div>
        </aside>


        <section class="is-title-bar">
            <div class="flex flex-col md:flex-row items-center justify-between space-y-6 md:space-y-0">
                <ul>
                    <li>Admin</li>
                    <li>Adminssions</li>
                </ul>
            </div>
        </section>

        <section class="is-hero-bar">
            <div class="flex flex-col md:flex-row items-center justify-between space-y-6 md:space-y-0">
                <h1 class="title">Adminssions </h1>
            </div>
        </section>

        <section class="section main-section">
            <div class="adminssion_table">
                <table>
                    <thead>
                        <tr>
                            <th>Title</th>
                            <th>Value</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th>Full name</th>
                            <td><%= adminssion.dataValues.adminssion_user_fullname %></td>
                        </tr>
                        <tr>
                            <th>Email</th>
                            <td><%= adminssion.dataValues.adminssion_user_email %></td>
                        </tr>
                        <tr>
                            <th>Phone number</th>
                            <td><%= adminssion.dataValues.adminssion_user_phone %></td>
                        </tr>
                        <tr>
                            <th>Birthdate</th>
                            <td><%= adminssion.dataValues.adminssion_user_birthdate %></td>
                        </tr>
                        <tr>
                            <th>Region</th>
                            <td><%= adminssion.dataValues.adminssion_user_region %></td>
                        </tr>
                        <tr>
                            <th>Address</th>
                            <td><%= adminssion.dataValues.adminssion_user_address %></td>
                        </tr>
                        <tr>

                        <tr>
                            <th>Branch</th>
                            <td><%= adminssion.dataValues.branch.dataValues.branch_name_eng %></td>
                        </tr>
                        <tr>
                            <th>Grade</th>
                            <td><%= adminssion.dataValues.adminssion_user_grade %></td>
                        </tr>
                        <tr>
                            <th>English degree</th>
                            <td><%= adminssion.dataValues.adminssion_user_english_degree %></td>
                        </tr>

                        <th>Parents full name</th>
                        <td><%= adminssion.dataValues.adminssion_user_partents_fullname %></td>
                        </tr>
                        <tr>
                            <th>Parents phone number</th>
                            <td><%= adminssion.dataValues.adminssion_user_partents_phone %></td>
                        </tr>
                        <tr>
                            <th>User Grade certificate</th>
                            <td>
                                <a class="button small blue"
                                    href="/files/<%= adminssion.adminssion_user_grade_certificate %>"
                                    download="<%= adminssion.adminssion_user_grade_certificate %>">Download</a>
                            </td>
                        </tr>
                        <tr>
                            <th>User Passport</th>
                            <td>
                                <a class="button small blue" href="/files/<%= adminssion.adminssion_user_passport %>"
                                    download="<%= adminssion.adminssion_user_passport %>">Download</a>
                            </td>
                        </tr>

                        <tr>
                            <th>User Parents Passport</th>
                            <td>
                                <a class="button small blue"
                                    href="/files/<%= adminssion.adminssion_user_partents_passport %>"
                                    download="<%= adminssion.adminssion_user_partents_passport %>">Download</a>
                            </td>
                        </tr>

                    </tbody>

                </table>
            </div>


        </section>

        <footer class="footer">
            <div class="flex flex-col md:flex-row items-center justify-between space-y-3 md:space-y-0">
                <div class="flex items-center justify-start space-x-3">
