    <div class="admission-form" id="admission" data-aos="fade-up">
      <div class="form__applications container">
        <div>
          <h2>
            <%= admissionLanguage.admission_form.title[lan] %>
          </h2>
        </div>
        <div class="alert alert-success"></div>
        <div class="alert alert-warn"></div>
        <div class="alert alert-error"></div>
        <form class="admission_form">
          <div class="application_atimes d-lg-flex justify-content-between">
            <div class="input__applications">
            
              <div class="card-form">
                <% var branchs = branchs || []; %>
                <div>
                  <select placeholder="Select breanch" class="select_branch" name="branch_id" required>
                    <option selected disabled>
                      <%= admissionLanguage.admission_form.form.branch[lan] %>
                    </option>
                    <% branchs.forEach(function(branch){ %>
                    <option value="<%= branch.branch_id %>">
                      <% if(lan === "eng") { %> 
                        <%= branch.branch_name_eng %>
                    <% } %>
                    <% if(lan === "ru") { %> 
                        <%= branch.branch_name_ru %>
                    <% } if(lan === "uz") {%>
                        <%= branch.branch_name_uz %>
                    <% } %> 
                    </option>
                    <% }) %>
                  </select>
                </div>
                <div class="input">
                  <input name="adminssion_user_fullname" type="text" class="input-field" required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.full_name[lan] %>
                  </label>
                </div>
                <div class="input">
                  <input name="adminssion_user_email" type="text" class="input-field" required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.email[lan] %>
                  </label>
                </div>
             
                <div class="input">
                  <input name="adminssion_user_phone" type="number" class="input-field" required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.mobil[lan] %>
                  </label>
                </div>
                <div class="input">
                  <input type="text" onfocus="(this.type='date')" class="input-field" name="adminssion_user_birthdate"
                    required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.birthdate[lan] %>
                    * (DD/MM/YYYY)</label>
                </div>
              </div>
              <div>
                <select placeholder="Select Product ID Type" class="apply_grade" name="adminssion_user_grade" required>
                  <option selected disabled>
                    <%= admissionLanguage.admission_form.form.grade[lan] %>
                  </option>
                </select>
              </div>
              <div>
                <select class="select english__new" name="adminssion_user_english_degree" required>
                  <option disabled selected>
                    <%= admissionLanguage.admission_form.form.english_level[lan] %>
                  </option>
                  <option value="Beginner">Beginner</option>
                  <option value="Elementary">Elementary</option>
                  <option value="Pre Intermediate">Pre Intermediate</option>
                  <option value="Intermediate">Intermediate</option>
                  <option value="Upper Intermediate">
                    Upper Intermediate
                  </option>
                  <option value="Advanced">Advanced</option>
                  <option value="Proficient">Proficient</option>
                </select>
              </div>
              <div>
                <select class="select region" name="adminssion_user_region" required>
                  <option selected disabled>
                    <%= admissionLanguage.admission_form.form.region[lan] %>
                  </option>
                  <option value="Andijan">Andijan</option>
                  <option value="Bukhara">Bukhara</option>
                  <option value="Fergana">Fergana</option>
                  <option value="Jizzakh">Jizzakh</option>
                  <option value="Xorazm">Xorazm</option>
                  <option value="Namangan">Namangan</option>
                  <option value="Navoiy">Navoiy</option>
                  <option value="Qashqadaryo">Qashqadaryo</option>
                  <option value="Samarqand">Samarqand</option>
                  <option value="Sirdaryo">Sirdaryo</option>
                  <option value="Surxondaryo">Surxondaryo</option>
                  <option value="Tashkent">Tashkent</option>
                  <option value="Karakalpakstan">Karakalpakstan</option>
                </select>
              </div>
              <div class="input">
                <input type="text" class="input-field" name="adminssion_user_address" required autocomplete="off"/>
                <label class="input-label">
                  <%= admissionLanguage.admission_form.form.address[lan] %>
                </label>
              </div>
              <div action="card-form">
                <div class="input">
                  <input type="text" class="input-field" name="adminssion_user_partents_fullname" required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.parents_fullname[lan] %>
                  </label>
                </div>
                <div class="input">
                  <input type="number" class="input-field" name="adminssion_user_partents_phone" required autocomplete="off"/>
                  <label class="input-label">
                    <%= admissionLanguage.admission_form.form.parents_mobile[lan] %>
                  </label>
                </div>
              </div>
            </div>
