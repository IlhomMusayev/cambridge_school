    <section class="news-header">
        <video autoplay muted loop id="myVideo">
            <source src="../assets/videos/Pexels Videos 2002527.mp4" type="video/mp4">
        </video>

        <div class="news-header__slide container">
            <div class="swiper newsHeaderSwiper">
               
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="news-header__data row d-flex justify-content-between align-items-center">
                            <div class="news-header__data__image col-lg-6">
                                <img src="../files/<%= newItem.new_image %>" alt="new images">
                            </div>
                            <div class="news-header__data__content col-lg-6">
                                <div class="date">
                                    <h3><%= moment( newItem.createdAt).format('d'); %></h3>
                                    <span> <%= moment( newItem.createdAt).format('MMM YYYY'); %> </span>
                                </div>
                                <hr>
                                <h2 class="title">
                                    <% if(lan === "uz") {%>
                                        <%= newItem.new_title_uz %>
                                        <% } if(lan === "ru") { %>
                                        <%= newItem.new_title_ru %>
                                        <% } else {%>
                                        <%= newItem.new_title_eng %>
                                        <% } %>
                                </h2>
                                <p>
                                    <% if(lan === "uz") {%>
                                        <%= newItem.new_content_uz %>
                                        <% } if(lan === "ru") { %>
                                        <%= newItem.new_content_ru %>
                                        <% } else {%>
                                        <%= newItem.new_content_eng %>
                                        <% } %>
                                </p>
                            </div>

                        </div>
                    </div>
                    
                </div>
                <div class="swiper-button-next">
                    <span>
                        Go To Next New
                    </span>
                </div>
                <div class="swiper-button-prev">
                    <span>
                        Back
                    </span>
                </div>
            </div>
        </div>
    </section>

    <section class="news container" data-aos="fade-up">
        <div class="news__title">
            <h2>
                <%= newsLanguage.last_news.news_title[lan] %>
            </h2>
        </div>
        <% var lastNews = lastNews || [] %> 
        <ul class="news__list container" >
            <div class="swiper newsSwiper">
                <div class="swiper-wrapper">
                    <% lastNews.forEach(function(element) { %>
                    <a href="/news/<%= element.new_slug %>" class="swiper-slide">
                        <div class="news__item">
                            <div class="background__image">
                                <img src="/files/<%= element.new_image %>" alt="image">
                            </div>
                            <div class="news__data">
                                <h3 class="title">
                                    <% if(lan === "uz") {%>
                                    <%= element.new_title_uz %>
                                    <% } if(lan === "ru") { %>
                                    <%= element.new_title_ru %>
                                    <% } else {%>
                                    <%= element.new_title_eng %>
                                    <% } %>
                                </h3>
                                <hr class="line">
                                </hr>
                                <p class="subtitle">
                                    <% if(lan === "uz") {%>
                                        <%= element.new_subtitle_uz %>
                                        <% } if(lan == "ru") { %>
                                        <%= element.new_subtitle_ru %>
                                        <% } else {%>
                                        <%= element.new_subtitle_eng %>
                                    <% } %>
                                </p>
                            </div>
                            <span class="created__data">
                                <h4><%= moment( element.createdAt).format('d'); %></h4>
                                <p> <%= moment( element.createdAt).format('MMM YYYY'); %> </p>
                            </span>
                        </div>
                    </a>
                    <% }); %>
                </div>
                <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div>
            </div>
        </ul>
    </section>  
    
    <% var allNews = allNews || [] %> 
    <section class="all_news container" data-aos="fade-up">
        <h2 class="all_news__title">
            <%= newsLanguage.all_news.news_title[lan] %>
        </h2>
        <ul class="all_news__list ">
            <% allNews.forEach(function(news){ %>
            <li class="all_news__list__item d-lg-flex align-items-center">
                <div class="all_news__list__item__date d-flex">
                    <div class="time">
                        <h3><%= moment( news.createdAt).format('d'); %></h3>
                        <span> <%= moment( news.createdAt).format('MMM YYYY'); %> </span>
                    </div>
                    <div class="all_news__list__item__image">
                        <img src="../files/<%= news.new_image %>" alt="news image">
                    </div>
                </div>
                <div class="all_news__list__item__content d-flex align-items-center">
                    <div class="all_news__list__item__content__text">
                        <h2>
                            <% if(lan === "uz") {%>
